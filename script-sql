/*CREAÇÃO DA DATABASE*/
CREATE DATABASE AgTurismo;

/*CRIAÇÃO DE TABELAS */
CREATE TABLE PacViagem (
  CodPacote INT AUTO_INCREMENT PRIMARY key,
  valor MONEY,
  duracao INT
)

CREATE TABLE PacGuia (
  CodRelacao INT AUTO_INCREMENT PRIMARY KEY,
  CodPacote INT,
  CodGuia INT
) 

CREATE TABLE PacPontoTuristico (
  CodRelacao INT AUTO_INCREMENT PRIMARY KEY,
  CodPacote INT,
  CodPontoTuristico INT
)

CREATE TABLE PacHoteis (
  CodRelacao INT AUTO_INCREMENT PRIMARY KEY,
  CodPacote INT,
  CodHoteis INT
)
  
CREATE TABLE PacCidades (
    CodRelacao INT AUTO_INCREMENT PRIMARY key,
    CodPacote INT,
    CodCidade INT
 )
 
CREATE TABLE GuiasTuristicos (
   CodGuia INT AUTO_INCREMENT PRIMARY key,
   nome VARCHAR(20),
   idade INT,
   CodCidade INT
 )
 
CREATE TABLE GuiaEspecialidades (
   CodRelacao INT AUTO_INCREMENT PRIMARY KEY,
   CodGuia INT,
   CodEspec INT
 )
 
CREATE TABLE cidades (
   CodCidade INT AUTO_INCREMENT PRIMARY key,
   cidade VARCHAR(50),
   estado VARCHAR(100),
   pais VARCHAR(50),
   populacao INT,
   CodClima INT
 )
 
CREATE TABLE GuiasEnderecos (
   CodRelacao INT AUTO_INCREMENT PRIMARY KEY,
   CodGuia INT,
   CodEndereco INT
 )
 
CREATE TABLE Especialidade (
   CodEspec INT AUTO_INCREMENT PRIMARY KEY,
   tipo VARCHAR(30)
 )
 
CREATE TABLE Enderecos (
   CodEnderecos INT AUTO_INCREMENT PRIMARY KEY,
   cep VARCHAR(9),
   logradouro VARCHAR(100)
 )
 
CREATE TABLE PontoTuristico (
   CodPontoTuristico INT AUTO_INCREMENT PRIMARY key,
   nome VARCHAR(100),
   cep VARCHAR(9),
   logradouro VARCHAR(100),
   PrecisaPagamento bool,
   HorarioInicio TIME,
   HorarioTermino TIME,
   CodCidade INT
 )
 
CREATE TABLE Hoteis (
   CodHoteis INT AUTO_INCREMENT PRIMARY KEY,
   Nome VARCHAR(50),
   Cep VARCHAR(9),
   logradouro VARCHAR(100),
   estrelas INT,
   NumeroQuartos INT,
   CodCidade INT
 )
 
CREATE TABLE Restaurantes (
   CodRestaurante INT AUTO_INCREMENT PRIMARY KEY,
   nome VARCHAR(50),
   cep VARCHAR(9),
   logradouro VARCHAR(100),
   estrelas INT,
   TipoComida VARCHAR(50),
   CodCidade INT
 )
 
CREATE TABLE Localidades (
   CodLocalidade INT AUTO_INCREMENT PRIMARY KEY,
   cep VARCHAR(9),
   cidade VARCHAR(50),
   estado VARCHAR(100),
   pais VARCHAR(50)
 )
   
